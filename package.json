{
  "name": "@dojo/parade",
  "version": "0.0.3-pre",
  "scripts": {
    "dev": "dojo build app -m dev -w -s -f docs=true",
    "start": "npm run dev",
    "dist": "dojo build widget -t lib",
    "dist:legacy": "dojo build widget -t lib --legacy",
    "copy": "mkdir -p ./dist/release && cp -r ./output/dist/* ./dist/release/",
    "clean": "rm -rf ./output/dist && rm -rf output",
    "package": "npm run clean && npm run dist && npm run copy && npm run dist:legacy && npm run copy",
    "release": "dojo-release"
  },
  "peerDependencies": {
    "@dojo/framework": "next",
    "@dojo/themes": "^6.0.0",
    "@dojo/widgets": "next"
  },
  "dependencies": {
    "canonical-path": "1.0.0",
    "cldr-data": "36.0.0",
    "intern": "4.6.0",
    "postcss": "7.0.21",
    "prismjs": "1.17.1",
    "rehype-stringify": "6.0.0",
    "remark-parse": "7.0.1",
    "remark-rehype": "5.0.0",
    "ts-morph": "4.2.0",
    "tslib": "~1.9.1",
    "unified": "8.4.1"
  },
  "devDependencies": {
    "@dojo/cli": "^7.0.0-alpha.1",
    "@dojo/cli-build-app": "next",
    "@dojo/cli-build-widget": "next",
    "@dojo/framework": "next",
    "@dojo/scripts": "^4.0.2",
    "@dojo/themes": "^6.0.0",
    "@dojo/widgets": "next",
    "@types/node": "~9.6.5",
    "shx": "^0.3.2",
    "typescript": "~3.4.5"
  }
}
